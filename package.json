{
  "name": "echo",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run build:frontend && concurrently \"npm run start:backend\" \"npm run serve",
    "start:backend": "cd Backend && npm start",
    "start:frontend": "cd Frontend && npm start",
    "install": "cd Backend && npm install && cd ../Frontend && npm install",
    "build:frontend": "cd Frontend && npm run build",
    "serve": "cd Frontend && http-server dist/frontend/browser"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "chart.js": "^4.4.4",
    "ng2-charts": "^6.0.1"
  },
  "devDependencies": {
    "@types/youtube": "^0.1.0",
    "concurrently": "^9.0.1",
    "dotenv": "^16.4.5",
    "prettier": "^3.3.2",
    "http-server": "^14.0.0"
  }
}